{
  "name": "javareact/xinge",
  "type": "library",
  "description": "腾讯移动信鸽推送SDK(PHP版本), Xinge API PHP Client",
  "homepage": "https://github.com/javareact/xinge",
  "license": "MIT",
  "require": {
    "php": ">=7.2",
    "ext-curl": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  },
  "autoload": {
    "psr-4": {
      "Javareact\\Xinge\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Xinge\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0-dev"
    }
  }
}
